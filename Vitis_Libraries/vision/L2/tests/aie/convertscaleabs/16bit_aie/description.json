{
    "name": "Xilinx convertscaleabs AIE Test", 
    "description": "convertscaleabs AIE function example", 
    "flow": "versal", 
    "platform_allowlist": [
        "vck190"
    ], 
    "platform_blocklist": [], 
    "platform_properties": {
        "vck190": {
            "v++": {
                "compiler": {
                    "clflags": [
                        ""
                    ]
                }, 
                "linker": {
                    "ldclflags": [
                        "--config PROJECT/system.cfg"
                    ]
                }, 
                "package": {
                    "clflags": [
                        "--package.defer_aie_run"
                    ]
                }
            }
        }
    }, 
    "gui": false, 
    "launch": [
        {
            "name": "hwemu-launch", 
            "target": "hw_emu"
        }, 
        {
            "name": "aiesim-launch", 
            "target": "aiesim"
        }, 
        {
            "name": "x86-launch", 
            "target": "x86sim"
        }
    ], 
    "host": {
        "host_exe": "host.elf", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/aie/convertscaleabs/16bit_aie/host.cpp", 
                "./Work/ps/c_rts/aie_control_xrt.cpp"
            ], 
            "includepaths": [
                "$(SYSROOT)/usr/include/xrt/", 
                "LIB_DIR/L1/include/aie", 
                "./"
            ], 
            "options": "--sysroot=$(SYSROOT)", 
            "symbols": [
                "__PS_ENABLE_AIE__"
            ]
        }, 
        "linker": {
            "librarypaths": [
                "$(SYSROOT)/usr/lib/"
            ], 
            "options": "--sysroot=$(SYSROOT)"
        }
    }, 
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/aie"
            ]
        }
    }, 
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/aie/convertscaleabs/16bit_aie/graph.cpp", 
                    "pl-freq": 300, 
                    "workdir": "./Work", 
                    "options": "-pl-axi-lite=true", 
                    "name": "libadf.a", 
                    "aie_kernel_files": [
                        "xf_convertscaleabs.cc"
                    ]
                }
            ]
        }
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/aie/convertscaleabs/16bit_aie/mm2s.cpp", 
                    "frequency": 300.0, 
                    "clflags": " ", 
                    "name": "mm2s"
                }, 
                {
                    "location": "LIB_DIR/L2/tests/aie/convertscaleabs/16bit_aie/s2mm.cpp", 
                    "frequency": 300.0, 
                    "clflags": " ", 
                    "name": "s2mm"
                }
            ], 
            "name": "kernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": {
                    "vitis_aie_sim": 55, 
                    "vitis_aie_x86sim": 55, 
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 200, 
                    "vitis_hw_run": 30
                }
            }
        ], 
        "targets": [
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}