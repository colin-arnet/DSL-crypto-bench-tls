{
    "gui": true,
    "name": "Xilinx RC4 Encryption Benchmark",
    "description": "RC4 Encryption Algorithm Benchmarking",
    "flow": "vitis",
    "platform_type": "",
    "platform_allowlist": [
        "u250"
    ],
    "platform_blocklist": [
        "zc",
        "aws-vu9p-f1"
    ],
    "platform_properties": {
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        },
        "vck190": {
            "host": {
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib",
                        "${SYSROOT}/opt/xilinx/xrt/lib"
                    ]
                },
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include"
                    ],
                    "options": "--sysroot=$(SYSROOT)"
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " -mode fpga -xclbin BUILD/rc4EncryptKernel.xclbin -gld PROJECT/data/gld.dat",
            "name": "generic launch for all flows"
        }
    ],
    "data": [
        "PROJECT/data/gld.dat"
    ],
    "host": {
        "host_exe": "rc4EncryptBenchmarck.exe",
        "compiler": {
            "sources": [
                "host/main.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L1/include",
                "host",
                "kernel",
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ]
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include",
                "kernel"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/rc4EncryptKernel1.cpp",
                    "frequency": 200.0,
                    "name": "rc4EncryptKernel_1",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "rc4EncryptKernel_1",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "inputData",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "outputData",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 150.0,
            "name": "rc4EncryptKernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 65536,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 1440,
                    "vitis_hw_emu": 470,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
